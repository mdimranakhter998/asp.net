<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EcommerceUI</title>

   
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
   @*  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />*@
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/jquery.dataTables.min.css" asp-append-version="true" />  
    <link rel="stylesheet" href="~/css/all.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/animate.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/flaticon.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/ionicons.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/linearicons.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/magnific-popup.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/responsive.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/simple-line-icons.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/slick.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/slick-theme.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/themify-icons.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/owlcarousel/css/owl.carousel.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/owlcarousel/css/owl.theme.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/owlcarousel/css/owl.theme.default.min.css" asp-append-version="true" />
   
    




</head>
<body>
    <partial name="Common/_Header.cshtml">
   @* <div class="container">
       
    </div>*@
        <main role="main">
            @RenderBody()
        </main>

    <partial name="Common/_Footer.cshtml">
  
  
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/imagesloaded.pkgd.min.js" asp-append-version="true"></script>
        <script src="~/js/isotope.min.js" asp-append-version="true"></script>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/js/jquery.countdown.min.js" asp-append-version="true"></script>
        <script src="~/js/jquery.dd.min.js" asp-append-version="true"></script>
        <script src="~/js/jquery.elevatezoom.js" asp-append-version="true"></script>
        <script src="~/js/magnific-popup.min.js" asp-append-version="true"></script>
        <script src="~/js/parallax.js" asp-append-version="true"></script>
        <script src="~/js/popper.min.js" asp-append-version="true"></script>
        <script src="~/js/scripts.js" asp-append-version="true"></script>
        <script src="~/js/slick.min.js" asp-append-version="true"></script>
        <script src="~/js/waypoints.min.js" asp-append-version="true"></script>
            <script src="~/owlcarousel/js/owl.carousel.min.js" asp-append-version="true"></script>


    @await RenderSectionAsync("Scripts", required: false)
